{
  "name": "hannaex",
  "version": "1.0.0",
  "description": "Bot de Discord",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "copy:assets": "copyfiles -u 1 \"src/**/*.{json,sql,env,txt,md,png,jpg,jpeg,webp,svg,ico}\" dist",
    "postbuild": "npm run copy:assets",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "deploy": "npm run build && node dist/scripts/deploy-commands.js",
    "deploy:global": "npm run build && node dist/scripts/deploy-commands.js --global"
  },
  "dependencies": {
    "@discord-player/opus": "^7.1.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@google/generative-ai": "^0.24.1",
    "@napi-rs/canvas": "^0.1.90",
    "@snazzah/davey": "^0.1.7",
    "axios": "^1.12.2",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "ffmpeg-static": "^5.2.0",
    "genius-lyrics": "^4.4.7",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.1",
    "ms": "^2.1.3",
    "play-dl": "^1.9.7",
    "pm2": "^6.0.14",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.10.9",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
